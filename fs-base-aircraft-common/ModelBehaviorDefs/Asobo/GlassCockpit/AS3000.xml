<ModelBehaviors>
	<Include ModelBehaviorFile="Asobo\Generic.xml"/>

	<!-- generic templates for AS3000 -->
	<!-- 
		DISPLAY_POS: 	L, M, R
		DISPLAY: 		PFD, MFD
	-->
	<Template Name="ASOBO_AS3000_Softkey_Template">
		<DefaultTemplateParameters>
			<PREFIX>#AS3000#_#DISPLAY#</PREFIX>
			<Switch Param="DISPLAY">
				<Case Value="PFD">
					<KEY_PREFIX>#AS3000#_PFD_#ID#</KEY_PREFIX>
				</Case>
				<Case Value="MFD">
					<KEY_PREFIX>#AS3000#_MFD</KEY_PREFIX>
				</Case>
			</Switch>
			<Condition>
				<Test>
					<Lower>
						<Value>KEY_ID</Value>
						<Number>10</Number>
					</Lower>
				</Test>
				<True>
					<UID>0#KEY_ID#</UID>
				</True>
				<False>
					<UID>#KEY_ID#</UID>
				</False>
			</Condition>
		</DefaultTemplateParameters>
		<UseTemplate Name="ASOBO_AS3000_Softkey_SubTemplate"></UseTemplate>
	</Template>
	<Template Name="ASOBO_AS3000_Softkey_SubTemplate">
		<DefaultTemplateParameters>
			<NODE_ID>#AS3000#_Push_#DISPLAY_POS#_#UID##SUFFIX_ID#</NODE_ID>
			<ANIM_NAME>#AS3000#_Push_#DISPLAY_POS#_#UID##SUFFIX_ID#</ANIM_NAME>
			<PART_ID>#PREFIX#_Push_Softkey_#KEY_ID#</PART_ID>
			<WWISE_EVENT_1>as3000_push_button_on</WWISE_EVENT_1>  
			<NORMALIZED_TIME_1>0.1</NORMALIZED_TIME_1>
			<WWISE_EVENT_2>as3000_push_button_off</WWISE_EVENT_2> 
			<NORMALIZED_TIME_2>0.5</NORMALIZED_TIME_2>	
		</DefaultTemplateParameters>
		<Component ID="#NODE_ID#_#ID#" Node="#NODE_ID#">
			<UseTemplate Name="ASOBO_GT_Emissive_Gauge"/>
			<UseTemplate Name="ASOBO_GT_Push_Button">
				<LEFT_SINGLE_CODE>(&gt;H:#KEY_PREFIX#_SOFTKEYS_#KEY_ID#)</LEFT_SINGLE_CODE>       
			</UseTemplate>
		</Component>
	</Template>
	<Template Name="ASOBO_AS3000_12_Softkeys_Template">
		<DefaultTemplateParameters>
			<ID>1</ID>
		</DefaultTemplateParameters>
		
		<UseTemplate Name="ASOBO_GT_Helper_Suffix_ID_Appender">
			<TEMPLATE_TO_CALL>ASOBO_AS3000_12_Softkeys_SubTemplate</TEMPLATE_TO_CALL>
		</UseTemplate>
	</Template>

	<Template Name="ASOBO_AS3000_12_Softkeys_SubTemplate">
		<UseTemplate Name="ASOBO_AS3000_Softkey_Template"><KEY_ID>1</KEY_ID></UseTemplate>
		<UseTemplate Name="ASOBO_AS3000_Softkey_Template"><KEY_ID>2</KEY_ID></UseTemplate>
		<UseTemplate Name="ASOBO_AS3000_Softkey_Template"><KEY_ID>3</KEY_ID></UseTemplate>
		<UseTemplate Name="ASOBO_AS3000_Softkey_Template"><KEY_ID>4</KEY_ID></UseTemplate>
		<UseTemplate Name="ASOBO_AS3000_Softkey_Template"><KEY_ID>5</KEY_ID></UseTemplate>
		<UseTemplate Name="ASOBO_AS3000_Softkey_Template"><KEY_ID>6</KEY_ID></UseTemplate>
		<UseTemplate Name="ASOBO_AS3000_Softkey_Template"><KEY_ID>7</KEY_ID></UseTemplate>
		<UseTemplate Name="ASOBO_AS3000_Softkey_Template"><KEY_ID>8</KEY_ID></UseTemplate>
		<UseTemplate Name="ASOBO_AS3000_Softkey_Template"><KEY_ID>9</KEY_ID></UseTemplate>
		<UseTemplate Name="ASOBO_AS3000_Softkey_Template"><KEY_ID>10</KEY_ID></UseTemplate>
		<UseTemplate Name="ASOBO_AS3000_Softkey_Template"><KEY_ID>11</KEY_ID></UseTemplate>
		<UseTemplate Name="ASOBO_AS3000_Softkey_Template"><KEY_ID>12</KEY_ID></UseTemplate>
	</Template>

	<!-- AS3000 screen template -->
	<Template Name="ASOBO_AS3000_Screen_Template">
		<DefaultTemplateParameters>
			<POTENTIOMETER>4</POTENTIOMETER>
			<SCREEN_LIGHT_DIMMING_SCALE>0.5</SCREEN_LIGHT_DIMMING_SCALE>
			<SCREEN_MIN_EMISSIVE_PERCENT>0.05</SCREEN_MIN_EMISSIVE_PERCENT>
			<MIN_AMBIENT_EMISSIVE_PERCENT>0.1</MIN_AMBIENT_EMISSIVE_PERCENT>
			<MAX_EMISSIVE_PERCENT>1</MAX_EMISSIVE_PERCENT>
			<INSTR_ID>1</INSTR_ID>
			<ID>1</ID>
			<AS3000>AS3000</AS3000>
		</DefaultTemplateParameters>
		
		<UseTemplate Name="ASOBO_GT_Helper_Suffix_ID_Appender">
			<TEMPLATE_TO_CALL>ASOBO_AS3000_Screen_SubTemplate</TEMPLATE_TO_CALL>
		</UseTemplate>
	</Template>

	<Template Name="ASOBO_AS3000_Screen_SubTemplate">
		<DefaultTemplateParameters>
			<Condition Check="SCREEN_NODE_ID">
				<True>
					<NODE_ID>#SCREEN_NODE_ID#</NODE_ID>
					<DISPLAY>Display</DISPLAY>
					<KEY_PREFIX>#AS3000#_Display_#INSTR_ID#</KEY_PREFIX>
				</True>
				<False>
					<Condition Check="DISPLAY">
						<True>
					<Switch Param="DISPLAY">
						<Case Value="PFD">
							<NODE_ID>#AS3000#_PFD#SUFFIX_ID#</NODE_ID>
							<KEY_PREFIX>#AS3000#_PFD_#INSTR_ID#</KEY_PREFIX>
						</Case>
						<Case Value="MFD">
							<NODE_ID>#AS3000#_MFD#SUFFIX_ID#</NODE_ID>
							<KEY_PREFIX>#AS3000#_MFD</KEY_PREFIX>
						</Case>
						<Default>
							<NODE_ID>#AS3000#_#DISPLAY##SUFFIX_ID#</NODE_ID>
							<KEY_PREFIX>#AS3000#_#DISPLAY#_#INSTR_ID#</KEY_PREFIX>
						</Default>
					</Switch>
						</True>
						<False>
							<DISPLAY>Display</DISPLAY>
							<NODE_ID>#AS3000#_Display_#ID#</NODE_ID>
							<KEY_PREFIX>#AS3000#_Display_#INSTR_ID#</KEY_PREFIX>
				</False>
			</Condition>
				</False>
			</Condition>
			<SCREEN_EMISSIVE_CODE>(A:LIGHT POTENTIOMETER:#POTENTIOMETER#, Percent over 100) #SCREEN_LIGHT_DIMMING_SCALE# * #MAX_EMISSIVE_PERCENT# #SCREEN_MIN_EMISSIVE_PERCENT# - * #SCREEN_MIN_EMISSIVE_PERCENT# +</SCREEN_EMISSIVE_CODE>
			<Condition Check="CIRCUIT_ID">
				<True>
					<FAILURE>(A:CIRCUIT ON:#CIRCUIT_ID#, Bool)</FAILURE>
				</True>
				<False>
					<FAILURE>(A:CIRCUIT GENERAL PANEL ON, Bool)</FAILURE>
				</False>
			</Condition>
		</DefaultTemplateParameters>
		<Condition Check="AMBIENT_POTENTIOMETER">
			<Component ID="#AS3000#_#DISPLAY#_Ambient_#INSTR_ID#_#ID#">
				<UseTemplate Name="ASOBO_LIGHTING_Ambient_Template">
					<BOUNCE_POTENTIOMETER_PERCENT>
						(L:#KEY_PREFIX#_ScreenLuminosity) (A:LIGHT POTENTIOMETER:#POTENTIOMETER#, Percent over 100) #MAX_EMISSIVE_PERCENT# #SCREEN_MIN_EMISSIVE_PERCENT# - * * #MIN_AMBIENT_EMISSIVE_PERCENT# + #FAILURE# (L:#KEY_PREFIX#_ScreenLuminosity) 0 &gt; and * 100 *
					</BOUNCE_POTENTIOMETER_PERCENT>
					<POTENTIOMETER>#AMBIENT_POTENTIOMETER#</POTENTIOMETER>
				</UseTemplate>
			</Component>
		</Condition>
		<Component ID="#AS3000#_#DISPLAY#_Screen_#INSTR_ID#_#ID#" Node="#NODE_ID#">
			<UseTemplate Name="ASOBO_GT_Emissive_Gauge">
				<PART_ID>#AS3000#_#DISPLAY#</PART_ID>
				<EMISSIVE_CODE>#SCREEN_EMISSIVE_CODE#</EMISSIVE_CODE>
			</UseTemplate>
		</Component>
	</Template>

	<!-- AS3000 general template -->
	<Template Name="ASOBO_AS3000_Template">
		<DefaultTemplateParameters>
			<AS3000>AS3000</AS3000>
			<MIN_EMISSIVE_PERCENT>0.02</MIN_EMISSIVE_PERCENT>
			<MAX_EMISSIVE_PERCENT>1</MAX_EMISSIVE_PERCENT>
			<LIGHT_DIMMING_SCALE>0.3</LIGHT_DIMMING_SCALE>
			<POTENTIOMETER>4</POTENTIOMETER>
			<STATIC_SCREEN_EMISSIVE_VALUE>0.5</STATIC_SCREEN_EMISSIVE_VALUE>
		</DefaultTemplateParameters>
		<UseTemplate Name="ASOBO_AS3000_12_Softkeys_Template">
			<Condition Check="USE_STATIC_SCREEN_EMISSIVE">
				<True>
					<EMISSIVE_CODE>(A:LIGHT POTENTIOMETER:#POTENTIOMETER#, Percent over 100)</EMISSIVE_CODE>
					<SCREEN_EMISSIVE_CODE>#STATIC_SCREEN_EMISSIVE_VALUE#</SCREEN_EMISSIVE_CODE>
				</True>
				<False>
					<EMISSIVE_CODE>(A:LIGHT POTENTIOMETER:#POTENTIOMETER#, Percent over 100) #LIGHT_DIMMING_SCALE# * #MAX_EMISSIVE_PERCENT# #MIN_EMISSIVE_PERCENT# - * #MIN_EMISSIVE_PERCENT# +</EMISSIVE_CODE>
				</False>
			</Condition>
		</UseTemplate>
		<UseTemplate Name="ASOBO_AS3000_Screen_Template">
			<Condition Check="USE_STATIC_SCREEN_EMISSIVE">
				<SCREEN_EMISSIVE_CODE>#STATIC_SCREEN_EMISSIVE_VALUE#</SCREEN_EMISSIVE_CODE>
			</Condition>
		</UseTemplate>
	</Template>

	<!-- AS3000_PFD -->
	<Template Name="ASOBO_AS3000_PFD">
		<DefaultTemplateParameters>
