					<DISPLAY_POS>L</DISPLAY_POS> 
				</Case>
				<Case Value="2">
					<DISPLAY_POS>R</DISPLAY_POS> 
				</Case>
			</Switch>
		</UseTemplate>
	</Template>

	<!-- AS3000_MFD -->
	<Template Name="ASOBO_AS3000_MFD">
		<DefaultTemplateParameters>
			<ID>1</ID>
			<POTENTIOMETER>4</POTENTIOMETER>
		</DefaultTemplateParameters>
		<UseTemplate Name="ASOBO_AS3000_Template">
			<DISPLAY>MFD</DISPLAY>
			<DISPLAY_POS>M</DISPLAY_POS>
