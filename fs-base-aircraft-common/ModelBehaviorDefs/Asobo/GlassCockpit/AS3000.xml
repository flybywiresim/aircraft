	<!-- AS580 -->
	<Template Name="ASOBO_AS580_Template">
		<DefaultTemplateParameters>
			<ID>1</ID>
			<MAX_EMISSIVE_PERCENT>1</MAX_EMISSIVE_PERCENT>
			<AS3000>AS580</AS3000>
			<ORIENTATION>Vertical</ORIENTATION>
