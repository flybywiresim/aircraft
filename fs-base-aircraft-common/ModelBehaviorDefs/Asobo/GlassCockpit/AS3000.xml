			<DISPLAY>MFD</DISPLAY>
			<TYPE></TYPE>
			<ID>1</ID>
			<ANIM_NEVER_FAILS/>
		</DefaultTemplateParameters>
		
		<UseTemplate Name="ASOBO_GT_Helper_Suffix_ID_Appender">
			<TEMPLATE_TO_CALL>ASOBO_AS3000_Detached_Control_Panel_SubTemplate</TEMPLATE_TO_CALL>
		</UseTemplate>
	</Template>
