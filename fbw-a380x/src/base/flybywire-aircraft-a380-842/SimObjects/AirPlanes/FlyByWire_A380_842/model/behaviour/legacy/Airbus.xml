    <Template Name="FBW_AUTOPILOT_Push_SubTemplate">
        <DefaultTemplateParameters>
            <Condition Check="NODE_ID">
                <True>
                    <BASE_NODE_ID>#NODE_ID#</BASE_NODE_ID>
                </True>
            </Condition>
            <ACTIVE_NODE_ID>#NODE_ID#_SEQ1</ACTIVE_NODE_ID>
